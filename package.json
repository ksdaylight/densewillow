        "build:nest-api:prod": "nx run nest-api:build:production",

        "build:admin": "nx run slash-admin:build",
        "build:admin:prod": "nx run slash-admin:build:production",

        "start:frontend:prod": "nx run frontend:serve:production --skip-nx-cache --port=4200",
